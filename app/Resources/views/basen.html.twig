<!DOCTYPE html>
<html>
    <head>
          <meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
          <title>{% block headtitle %}BLOG MROOT PROJET{% endblock %}</title>
          {% block stylesheets %}
        <link rel="stylesheet" href="{{ asset('bootstrap/css/bootstrap-responsive.css') }}" type="text/css" />
        <link rel="stylesheet" href="{{ asset('bootstrap/css/bootstrap.css') }}" type="text/css" />
      {#    <link rel="stylesheet" href="{{ asset('css/bootstrap.min.css') }}" type="text/css" />
   <link rel="stylesheet" href="{{ asset('css/font-awesome.min.css') }}" type="text/css" />#}
 <link rel="stylesheet" href="{{ asset('css/redmond/jquery-ui-1.10.2.custom.css') }}" type="text/css" media="all"/>
        {#<link rel="stylesheet" href="{{ asset('css/jquery-ui-1.9.2.custom.css') }}" type="text/css" media="all"/>
         #}
 <link rel="stylesheet" href="{{ asset('css/main.css') }}" type="text/css" media="all"/>
 <style>
body {
background-image:url(/images/ravenna.png);
}

</style>
  {% endblock %}


        </head>
        <body>
         <!-- ==============================================
                                      NAVIGATION BAR
           ================================================-->

            <div class="page-header">
             {{ block('NAVIGATION_BAR') }}
            </div>
         
            <div class="container-fluid">
                <div class="row-fluid">
                    <!-- ==============================================
                                      CONTENT
                     ================================================-->

                    <div class="span9">
                         <center><h1>   {% block title %}BLOG MROOT PROJET{% endblock %}</h1></center>
                        {% block content %}
                        {% endblock %}
                        </div>
                      <!-- ==============================================
                                            SIDEBAR 
                      ================================================-->

                        <div class="span3">
                              
              <div class="accordion" id="accordion2">
                  <div class="accordion-group">
<div class="accordion-heading">
<a class="accordion-toggle" data-toggle="collapse" data-parent="#accordion2" href="#collapseOne">
     <h4 class="ui-widget-header ui-corner-all">Tags</h4>
</a>
</div>
<div id="collapseOne" class="accordion-body collapse in">
<div class="accordion-inner">
    {% block tags %}
    {% endblock %}
</div>
</div>
</div>
<div class="accordion-group">
<div class="accordion-heading">
<a class="accordion-toggle" data-toggle="collapse" data-parent="#accordion2" href="#collapseTwo">
   <h4 class="ui-widget-header ui-corner-all">Commentaires</h4>
</a>
</div>
<div id="collapseTwo" class="accordion-body collapse">
<div class="accordion-inner">
  {% block comments %}
  {% endblock %}
</div>
</div>
</div>
                      <div class="accordion-group">
<div class="accordion-heading">
<a class="accordion-toggle" data-toggle="collapse" data-parent="#accordion2" href="#collapseThree">
   <h4 class="ui-widget-header ui-corner-all">Categories</h4>
</a>
</div>
<div id="collapseThree" class="accordion-body collapse">
<div class="accordion-inner">
  {% block categories %}
                                  {% endblock %}
</div>
</div>
</div>
                      
                      
                      
                        <div class="accordion-group">
<div class="accordion-heading">
<a class="accordion-toggle" data-toggle="collapse" data-parent="#accordion2" href="#collapse4">
   <h4 class="ui-widget-header ui-corner-all">Archives</h4>
</a>
</div>
<div id="collapse4" class="accordion-body collapse">
<div class="accordion-inner">
  {% block archives %}
                                  {% endblock %}
</div>
</div>
</div>
</div>
                       {% block sidebar %}
                        
                        {% endblock %}
                        </div>
                    </div>
                    <div id="footer">
                        <p align="center">{{ 'copyright MROOT'|trans }} Â© 2012</p>

                    </div>
                </div>
                    <!-- ==============================================
                                           FOOTER 
                     ================================================-->
                    
                 
                    <!-- Le javascript ================================================== -->
                    <!-- Placed at the end of the document so the pages load faster -->

       {% block javascripts %}
                    <script src="{{ asset('jquery/jquery-1.8.3.js') }}" type="text/javascript"></script>
                         <script type="text/javascript" src="{{ asset('jquery/jquery-ui-1.10.2.custom.min.js') }}"></script>
                    {#
<script type="text/javascript" src="{{ asset('jquery/jquery-ui-1.9.2.custom.min.js') }}"></script>
#}
                    <script type="text/javascript" src="{{ asset('jquery/jquery.ui.datepicker.js') }}"></script>
                    <script type="text/javascript" src="{{ asset('bootstrap/js/bootstrap.js') }}"></script>
                   {#   <script type="text/javascript" src="{{ asset('bootstrap/js/bootstrap.min.js') }}"></script>#}
  {% endblock %}
     {#{ tinymce_init() }#}

                </body>

            </html>


