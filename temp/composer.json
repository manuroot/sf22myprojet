{
    "name": "symfony/framework-standard-edition",
    "description": "The \"Symfony Standard Edition\" distribution",
    "autoload": {
        "psr-0": {
            "": "src/"
        }
    },
    "require": {
        "php": ">=5.3.3",
        "symfony/symfony": "2.2.*",
        "doctrine/orm": "~2.2,>=2.2.3",
        "doctrine/doctrine-bundle": "1.2.*",
        "twig/extensions": "1.0.*",
        "symfony/assetic-bundle": "2.1.*",
        "symfony/swiftmailer-bundle": "2.2.*",
        "symfony/monolog-bundle": "2.2.*",
        "sensio/distribution-bundle": "2.2.*",
        "sensio/framework-extra-bundle": "2.2.*",
        "sensio/generator-bundle": "2.2.*",
        "jms/security-extra-bundle": "1.4.*",
        "jms/di-extra-bundle": "1.3.*",
        
     
        "knplabs/gaufrette" : "0.2.*@dev",
        "knplabs/knp-gaufrette-bundle" : "dev-master",
        "knplabs/knp-paginator-bundle": "dev-master",
        "knplabs/knp-menu-bundle": "1.1.x-dev",
        "knplabs/knp-menu": "1.1.x-dev",
      
            
        "friendsofsymfony/user-bundle": "v1.3.1",
          
        "sonata-project/easy-extends-bundle" : "dev-master",
        "sonata-project/cache-bundle": "dev-master",
        "sonata-project/jquery-bundle": "1.8.3",
        "sonata-project/admin-bundle": "dev-master",
        "sonata-project/block-bundle": "dev-master",
        "sonata-project/doctrine-extensions": "1.0.0",
        "sonata-project/user-bundle": "dev-master",
        "sonata-project/doctrine-orm-admin-bundle": "dev-master",
        "sonata-project/notification-bundle": "dev-master",
     
        "sonata-project/media-bundle" : "dev-master",
        "sonata-project/formatter-bundle": "dev-master",
        "sonata-project/news-bundle": "dev-master",
        "sonata-project/intl-bundle" :"2.1.0",
        "sonata-project/google-authenticator": "1.0.0",
        "sonata-project/markitup-bundle" : "dev-master",
        
        
        "vich/uploader-bundle": "dev-master",
        "craue/formflow-bundle" :"dev-master",
        "doctrine/data-fixtures": "dev-master",
        "kriswallsmith/assetic": "dev-master",
        "mopa/composer-bridge": "dev-master",
        "mopa/bootstrap-bundle": "dev-master",
        "twitter/bootstrap": "dev-master",
        "braincrafted/bootstrap-bundle": "dev-master",
        "stfalcon/tinymce-bundle": "dev-master",
        "jms/payment-core-bundle": "dev-master",
        "jms/payment-paypal-bundle": "dev-master",
          "payum/payum-bundle": "0.5.x-dev",
        "payum/paypal-express-checkout-nvp": "0.5.x-dev",
        "payum/paypal-pro-checkout-nvp": "0.5.x-dev",
        "payum/authorize-net-aim": "0.5.x-dev",
        "payum/be2bill": "0.5.x-dev",
        "payum/omnipay-bridge": "0.5.x-dev",
       
        "beberlei/DoctrineExtensions": "dev-master",
        "lexik/form-filter-bundle": "dev-master",
        "savvy/filternator-bundle": "dev-master",
        
        /*test*/
        
        "payum/payum": "0.4.0",
       "payum/payum-bundle": "0.4.0",
        "payum/paypal-express-checkout-nvp": "0.4.*",
        "payum/paypal-pro-checkout-nvp": "0.4.*",
        "payum/authorize-net-aim": "0.4.*",
        "payum/be2bill": "0.4.*",
        "payum/omnipay-bridge": "0.4.*",
        
        
    },
    "scripts": {
          "pre-install-cmd": [
            "Application\\PaymentBundle\\Composer\\ScriptHandler::createParametersYaml"
        ],
        "post-install-cmd": [
        "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
              "Mopa\\Bundle\\BootstrapBundle\\Composer\\ScriptHandler::postInstallSymlinkTwitterBootstrapSass",
              "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
        "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
        "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
        ],
        "post-update-cmd": [
        "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
        "Mopa\\Bundle\\BootstrapBundle\\Composer\\ScriptHandler::postInstallSymlinkTwitterBootstrap",
        "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
        "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
        "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
        ]
    },
    "config": {
        "bin-dir": "bin"
    },
    "minimum-stability": "dev",
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web",
        "branch-alias": {
            "dev-master": "2.2-dev",
            "dev-master": "0.5-dev"
        }
    }
}



